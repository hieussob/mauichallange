<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ClothingFashion.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:ClothingFashion.Features.Home.Views"
    xmlns:local="clr-namespace:ClothingFashion"
    Shell.FlyoutBehavior="Disabled"
    Title="ClothingFashion">

    <ShellContent
        Title="HomePage"
        ContentTemplate="{DataTemplate views:HomePage}"
        Route="HomePage" />
    <ShellContent
        Title="MainPage"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

</Shell>
